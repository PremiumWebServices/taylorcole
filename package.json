  "main": "gulpfile.js",
  "dependencies": {
    "featherlight": "1.7.14",
    "jquery": "3.5.1",
    "jquery-match-height": "0.7.2",
    "slick-carousel": "1.8.1",
    "svg4everybody": "2.1.9"
  },
  "devDependencies": {
    "@babel/core": "7.4.*",
    "@babel/preset-env": "7.4.*",
    "babel-eslint": "^6.1.2",
    "babel-loader": "8.0.*",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.11.6",
    "browser-sync": "^2.13.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.5",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-jshint": "^2.0.1",
    "gulp-notify": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^8.1.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.0.9",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-svgmin": "^2.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.7",
    "gulp-wp-pot": "^2.4.2",
    "node-sass": "^4.11.0",
    "run-sequence": "^2.2.1",
    "stylefmt": "6.0.*",
    "stylelint": "9.2.*",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-no-unsupported-browser-features": "3.0.*",
    "stylelint-scss": "3.1.*"
  },
  "repository": {
    "type": "git",
    "url": "/.git"
  },
  "keywords": [],
  "globs": {
    "styles": {
      "in": "src/css/*.scss",
      "out": "./",
      "watch": "src/css/**/*.scss"
    },
    "scripts": {
      "in": "src/js",
      "out": "dist/js",
      "watch": "src/js/**/*.js"
    },
    "createRevisions": {
      "in": [
        "dist/**/*.min.css",
        "dist/**/*.min.js"
      ],
      "out": "dist/"
    },
    "images": {
      "in": "src/gui/**/*",
      "out": "dist/gui",
      "watch": ""
    },
    "svg": {
      "in": "src/svg/**/*",
      "out": "dist/svg",
      "watch": "src/svg/**/*.svg"
    },
    "fonts": {
      "in": "src/fonts/**/*",
      "out": "dist/fonts",
      "watch": "src/fonts/**/*"
    }
  },
  "stylelint": {
    "extends": "./_tests/stylelint.config.js"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
